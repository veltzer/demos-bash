# A set of functions to help with dealing with arrays

# A function that prints an array.
# This should be usable for sparese array too.
function array_print() {
	local -n array="$1"
	# declare -p array
	# echo "length of array is ${#array[@]}"
	for index in ${!array[@]}
	do
		echo $index/${array[$index]}
	done
}
